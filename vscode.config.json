{
    "window.zoomLevel": 0,
    "editor.fontSize": 18,
    "terminal.integrated.shell.windows": "C:\\Program Files\\Git\\bin\\bash.exe",
    "files.associations": {
        "*.vm": "html",
        "*.babel": "javascript"
    },
    "telemetry.enableCrashReporter": false,
    "workbench.colorTheme": "Visual Studio Dark",
